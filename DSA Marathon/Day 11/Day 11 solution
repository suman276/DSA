#include<iostream>
#include<string>
#include<stack>
using namespace std;

void check_string(string s){
    stack<int> val;
    for(int i=0; i<s.size(); i++){
        if(47<s[i]<58){
            val.push(s[i]);
        }
        else if(s[i]=='*'){
            int a2=val.top();
            val.pop();
            int a1=val.top();
            val.pop();
            int p1=a2*a1;
            val.push(p1);
            cout<<a2<<"*"<<a1<<" ";
        }
        else if( s[i]=='+'){
            int a2=val.top();
            val.pop();
            int a1=val.top();
            val.pop();
            int p1=a2+a1;
            val.push(p1);
            cout<<a2<<"+"<<a1<<" ";
        }
        else if(s[i]=='-'){
            int a2=val.top();
            val.pop();
            int a1=val.top();
            val.pop();
            int p1=a2-a1;
            val.push(p1);
            cout<<a2<<"-"<<a1<<" ";
        }
    }
 int a3=val.top();
cout<<a3<<endl;
 }


int main(){
    int cases;
     cin>>cases;
     while(cases){
         string c;
         cin>>c;
        // stack<char> s;
         check_string(c);
         cases--;
     }
     return 0;
}
