#include<iostream>
#include<string>
#include<algorithm>
using namespace std;

int main(){
    int cases;
    cin>>cases;
    while(cases>0){
        string a;
        string b;
        cin>>a;
        cin>>b;
        int flag=1;
        sort(a.begin(), a.end());
        sort(b.begin(), b.end());
        
        for(int i=0; i<a.size(); i++){
            if(a[i]!=b[i]){
                cout<<"NO"<<endl;
                flag--;
                break;
            }
        }
        if(flag==1)
        cout<<"YES"<<endl;
            cases--;
    }
    return 0;
}
